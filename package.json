{
	"name": "fulltilt-ts",
	"title": "Full Tilt Typescript",
	"version": "0.7.0",
	"description": "Standalone device orientation + device motion normalization and conversion library. Converted to Typescript from the original Javascript version.",
	"author": {
		"name": "Haris Skiadas"
	},
	"keywords": [
		"full tilt",
		"fulltilt",
		"deviceorientation",
		"devicemotion",
		"compass",
		"quaternions",
		"rotation",
		"matrix",
		"euler",
		"tait bryan",
		"angles"
	],
	"scripts": {
		"build": "rollup -c",
		"start": "webpack serve --open",
		"test": "jest"
	},
	"exports": {
		".": {
			"types": "./dist/Core.d.ts",
			"import": "./dist/fulltilt.esm.js",
			"require": "./dist/fulltilt.cjs.js"
		},
		"./orientation": {
			"types": "./dist/orientation.d.ts"
		}
	},
	"type": "module",
	"files": [
		"./dist"
	],
	"types": "./dist/Core.d.ts",
	"main": "./dist/fulltilt.cjs.js",
	"module": "./dist/fulltilt.esm.js",
	"browser": "./dist/fulltilt.umd.js",
	"devDependencies": {
		"@babel/cli": "^7.21.0",
		"@babel/core": "^7.21.3",
		"@babel/preset-typescript": "^7.21.0",
		"@rollup/plugin-commonjs": "^24.1.0",
		"@rollup/plugin-node-resolve": "^15.0.2",
		"@rollup/plugin-typescript": "^11.1.0",
		"@types/jest": "^29.5.0",
		"babel-jest": "^29.5.0",
		"filemanager-webpack-plugin": "^8.0.0",
		"jest": "^29.5.0",
		"jest-matcher-deep-close-to": "^3.0.2",
		"prettier": "2.8.4",
		"rollup": "^3.20.6",
		"ts-jest": "^29.0.5",
		"ts-loader": "^9.4.2",
		"tslib": "^2.5.0",
		"typescript": "^4.9.5",
		"webpack": "^5.76.2",
		"webpack-cli": "^5.0.1",
		"webpack-dev-server": "^4.12.0"
	}
}
